
      - name: <%= collection[:moonrank] %> - Fetch Alpha Art data
        uses: githubocto/flat@v3
        with:
          http_url: https://apis.alpha.art/api/v1/collection
          downloaded_filename: <%= collection[:moonrank] %>__alphaart.json
          axios_config: .github/alpha_axios_configs/<%= collection[:moonrank] %>.json
          postprocess: ./postprocess/alphaart.ts
